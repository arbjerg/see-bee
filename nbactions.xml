<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>CUSTOM-Update Child Versions</actionName>
        <displayName>Update Child Versions</displayName>
        <recursive>false</recursive>
        <goals>
            <goal>versions:update-child-modules</goal>
        </goals>
    </action>
    <action>
        <actionName>CUSTOM-Deploy</actionName>
        <displayName>Deploy</displayName>
        <goals>
            <goal>clean</goal>
            <goal>deploy</goal>
            <goal>-P</goal>
            <goal>release</goal>
            <goal>-Dmaven.test.skip=true</goal>
        </goals>
    </action>
    <action>
        <actionName>CUSTOM-Fast Build</actionName>
        <displayName>Fast Build</displayName>
        <goals>
            <goal>clean</goal>
            <goal>install</goal>
            <goal>-Dmaven.compile.fork=true</goal>
            <goal>-Dmaven.test.skip=true</goal>
            <goal>-Denforcer.skip=true</goal>
        </goals>
    </action>
</actions>
